account default
host smtp.gmail.com
port 465
auth on
user YOUR_USER_NAME
password YOUR_PASS_WORD
tls_trust_file /etc/ssl/certs/ca-certificates.crt
tls on
tls_starttls off
from YOUR_EMAIL_ADDRESS
#maildomain YOUR_MAIL_DOMAIN
#set_from_header on
add_missing_date_header on
auto_from on
aliases /etc/aliases


